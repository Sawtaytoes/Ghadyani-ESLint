{
  "name": "@ghadyani-eslint/eslint-plugin-rules",
  "version": "2.2.2-alpha.0",
  "description": "Custom rules for @ghadyani-eslint/core.",
  "scripts": {
    "lint": "eslint --ext=js,jsx .",
    "lint:fix": "eslint . --ext=js,jsx --fix",
    "pretest": "npm run lint",
    "test": "ava",
    "test:watch": "ava --watch",
    "format": "prettier 'lib/**/*.js' --trailing-comma all --single-quote --write",
    "postformat": "yarn lint:fix"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Kevin Ghadyani",
  "main": "lib/index.js",
  "dependencies": {
    "requireindex": "^1.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0"
  },
  "license": "ISC",
  "gitHead": "d84e34e19d1d68d7d8914b1386a518cbdc491494"
}
