{
  "name": "@ghadyani-eslint/eslint-plugin-arrow-body-parens",
  "version": "2.0.1",
  "description": "Rule to require parenthesis in arrow function body expressions.",
  "scripts": {
    "lint": "eslint --ext=js,jsx .",
    "lint:fix": "eslint . --ext=js,jsx --fix",
    "pretest": "npm run lint",
    "test": "ava",
    "test:watch": "ava --watch",
    "format": "prettier 'lib/**/*.js' --trailing-comma all --single-quote --write",
    "postformat": "npm run lint:fix",
    "commit": "git-cz",
    "commitmsg": "validate-commit-msg"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Berkeley Martinez",
  "main": "lib/index.js",
  "dependencies": {
    "requireindex": "~1.1.0"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-eslint": "^7.2.3",
    "eslint": "^4.5.0",
    "eslint-plugin-import": "^2.7.0"
  },
  "license": "ISC",
  "gitHead": "cb87c0eb000c6609c241d1004573be0e91c70e82"
}
