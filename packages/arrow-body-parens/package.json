{
  "name": "@ghadyani-eslint/eslint-plugin-arrow-body-parens",
  "version": "2.0.3",
  "description": "Rule to require parenthesis in arrow function body expressions.",
  "scripts": {
    "lint": "eslint --ext=js,jsx .",
    "lint:fix": "eslint . --ext=js,jsx --fix",
    "pretest": "npm run lint",
    "test": "ava",
    "test:watch": "ava --watch",
    "format": "prettier 'lib/**/*.js' --trailing-comma all --single-quote --write",
    "postformat": "yarn lint:fix"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Berkeley Martinez",
  "main": "lib/index.js",
  "dependencies": {
    "requireindex": "~1.1.0"
  },
  "devDependencies": {
    "ava": "^3.4.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1"
  },
  "license": "ISC",
  "gitHead": "f361c76c38d1ef370b553e74ad3931f940717355"
}
