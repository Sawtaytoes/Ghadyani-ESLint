{
  "name": "@ghadyani-eslint/eslint-plugin-arrow-body-parens",
  "version": "2.2.7",
  "description": "Rule to require parenthesis in arrow function body expressions.",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Kevin Ghadyani",
  "scripts": {
    "lint": "eslint --ext=js,jsx .",
    "lint:fix": "eslint . --ext=js,jsx --fix",
    "pretest": "npm run lint",
    "test": "ava",
    "test:watch": "ava --watch",
    "format": "prettier 'lib/**/*.js' --trailing-comma all --single-quote --write",
    "postformat": "yarn lint:fix"
  },
  "packageManager": "yarn@3.2.2",
  "exports": "./lib/index.js",
  "main": "lib/index.js",
  "dependencies": {
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@typescript-eslint/utils": "^5.31.0",
    "requireindex": "^1.2.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.9",
    "ava": "^4.3.1",
    "eslint": "^8.20.0",
    "eslint-plugin-import": "^2.26.0"
  },
  "license": "ISC",
  "gitHead": "d84e34e19d1d68d7d8914b1386a518cbdc491494"
}
