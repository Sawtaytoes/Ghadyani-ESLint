{
  "name": "@ghadyani-eslint/eslint-plugin-eslint-overrides",
  "version": "2.2.4",
  "description": "Rules to override ESLint, so I don't have to wait on PRs to get approved in the official repo.",
  "scripts": {
    "lint": "eslint --ext=js,jsx .",
    "lint:fix": "eslint . --ext=js,jsx --fix",
    "pretest": "npm run lint",
    "test": "ava",
    "test:watch": "ava --watch",
    "format": "prettier 'lib/**/*.js' --trailing-comma all --single-quote --write",
    "postformat": "yarn lint:fix"
  },
  "packageManager": "yarn@3.2.2",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Kevin Ghadyani",
  "main": "lib/index.js",
  "dependencies": {
    "eslint-utils": "^3.0.0",
    "functional-red-black-tree": "^1.0.1",
    "lodash": "^4.17.21",
    "requireindex": "^1.2.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.9",
    "eslint": "^8.20.0"
  },
  "license": "ISC",
  "gitHead": "d84e34e19d1d68d7d8914b1386a518cbdc491494"
}
